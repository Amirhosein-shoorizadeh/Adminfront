<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditCouponController" alignment="CENTER" spacing="10" prefWidth="400" prefHeight="300">
    <children>
        <Label text="Edit Coupon" />
        <TextField fx:id="codeField" promptText="Coupon Code" />
        <TextField fx:id="typeField" promptText="Type" />
        <TextField fx:id="valueField" promptText="Value" />
        <TextField fx:id="minPriceField" promptText="Min Price" />
        <TextField fx:id="userCountField" promptText="User Count" />
        <TextField fx:id="startDateField" promptText="Start Date" />
        <TextField fx:id="endDateField" promptText="End Date" />
        <HBox spacing="10">
            <Button fx:id="saveButton" text="Save" onAction="#handleSave" />
            <Button fx:id="backButton" text="Back" onAction="#handleBack" />
        </HBox>
    </children>
</VBox>