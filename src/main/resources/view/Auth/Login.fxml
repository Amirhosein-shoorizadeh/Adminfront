<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="600.0" prefWidth="800.0" stylesheets="@../../styles/Admin.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">

    <children>
        <VBox alignment="TOP_CENTER" layoutX="245.0" layoutY="57.0" prefHeight="487.0" prefWidth="310.0" spacing="20.0">

            <children>
                <Label alignment="CENTER" prefHeight="114.0" prefWidth="335.0" styleClass="label-dashboard" text="Admin Login">
                    <font>
                        <Font name="Candara Bold" size="31.0" />
                    </font>
                </Label>

                <TextField fx:id="PhoneTextField" prefHeight="37.0" prefWidth="434.0" promptText="Phone" styleClass="text-field">
                    <VBox.margin>
                        <Insets top="60.0" />
                    </VBox.margin>
                </TextField>

                <TextField fx:id="PasswordTextField1" prefHeight="38.0" prefWidth="443.0" promptText="Password" styleClass="text-field" />

                <Button fx:id="LoginButton" mnemonicParsing="false" prefHeight="46.0" prefWidth="465.0" styleClass="button2" text="Login">
                    <VBox.margin>
                        <Insets top="60.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</Pane>
